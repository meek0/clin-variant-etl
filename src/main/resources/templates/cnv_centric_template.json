{
  "index_patterns": [
    "clin_qa_blue_cnv_centric*",
    "clin_qa_green_cnv_centric*",
    "clin_staging_cnv_centric*",
    "clin_prod_cnv_centric*"
  ],
  "template": {
    "settings": {
      "number_of_shards": 3
    },
    "mappings": {
      "properties": {
        "chromosome": {
          "type": "keyword"
        },
        "sort_chromosome": {
          "type": "integer"
        },
        "start": {
          "type": "long"
        },
        "reference": {
          "type": "keyword"
        },
        "alternate": {
          "type": "keyword"
        },
        "end": {
          "type": "long"
        },
        "name": {
          "type": "keyword"
        },
        "type": {
          "type": "keyword"
        },
        "qual": {
          "type": "double"
        },
        "bc": {
          "type": "long"
        },
        "sm": {
          "type": "double"
        },
        "variant_type": {
          "type": "keyword"
        },
        "flags": {
          "type": "keyword",
          "index": false
        },
        "batch_id": {
          "type": "keyword"
        },
        "bioinfo_analysis_code": {
          "type": "keyword"
        },
        "calls": {
          "type": "long"
        },
        "cn": {
          "type": "integer"
        },
        "pe": {
          "type": "long"
        },
        "is_multi_allelic": {
          "type": "boolean"
        },
        "old_multi_allelic": {
          "type": "keyword",
          "index": false
        },
        "ciend": {
          "type": "long"
        },
        "cipos": {
          "type": "long"
        },
        "svlen": {
          "type": "long"
        },
        "reflen": {
          "type": "long"
        },
        "svtype": {
          "type": "keyword"
        },
        "filters": {
          "type": "keyword"
        },
        "service_request_id": {
          "type": "keyword"
        },
        "patient_id": {
          "type": "keyword"
        },
        "aliquot_id": {
          "type": "keyword"
        },
        "analysis_service_request_id": {
          "type": "keyword"
        },
        "sequencing_strategy": {
          "type": "keyword"
        },
        "genome_build": {
          "type": "keyword"
        },
        "analysis_code": {
          "type": "keyword"
        },
        "analysis_display_name": {
          "type": "keyword"
        },
        "affected_status": {
          "type": "boolean",
          "index": false
        },
        "affected_status_code": {
          "type": "keyword"
        },
        "family_id": {
          "type": "keyword"
        },
        "is_proband": {
          "type": "boolean"
        },
        "gender": {
          "type": "keyword"
        },
        "practitioner_role_id": {
          "type": "keyword"
        },
        "organization_id": {
          "type": "keyword"
        },
        "mother_id": {
          "type": "keyword"
        },
        "father_id": {
          "type": "keyword"
        },
        "mother_aliquot_id": {
          "type": "keyword",
          "index": false
        },
        "father_aliquot_id": {
          "type": "keyword",
          "index": false
        },
        "specimen_id": {
          "type": "keyword"
        },
        "sample_id": {
          "type": "keyword"
        },
        "genes": {
          "type": "nested",
          "properties": {
            "symbol": {
              "type": "keyword"
            },
            "refseq_id": {
              "type": "keyword"
            },
            "panels": {
              "type": "keyword"
            },
            "gene_length": {
              "type": "long"
            },
            "overlap_bases": {
              "type": "long"
            },
            "overlap_cnv_ratio": {
              "type": "double"
            },
            "overlap_gene_ratio": {
              "type": "double"
            },
            "overlap_exons": {
              "type": "long"
            },
            "hpo": {
              "type": "nested",
              "properties": {
                "hpo_term_id": {
                  "type": "keyword"
                },
                "hpo_term_label": {
                  "type": "keyword"
                },
                "hpo_term_name": {
                  "type": "keyword",
                  "index": false
                }
              }
            },
            "cosmic": {
              "type": "nested",
              "properties": {
                "tumour_types_germline": {
                  "type": "keyword"
                }
              }
            },
            "ddd": {
              "type": "nested",
              "properties": {
                "disease_name": {
                  "type": "keyword"
                }
              }
            },
            "omim": {
              "type": "nested",
              "properties": {
                "inheritance": {
                  "type": "keyword"
                },
                "inheritance_code": {
                  "type": "keyword"
                },
                "name": {
                  "type": "keyword"
                },
                "omim_id": {
                  "type": "keyword"
                }
              }
            },
            "orphanet": {
              "type": "nested",
              "properties": {
                "disorder_id": {
                  "type": "long"
                },
                "inheritance": {
                  "type": "keyword"
                },
                "panel": {
                  "type": "keyword"
                }
              }
            }
          }
        },
        "number_genes": {
          "type": "long"
        },
        "hash": {
          "type": "keyword"
        }
      }
    }
  }
}